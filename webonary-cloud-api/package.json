{
  "name": "webonary-cloud-api",
  "version": "0.1.0",
  "bin": {
    "webonary-cloud-api": "bin/webonary-cloud-api.js"
  },
  "scripts": {
    "apidoc": "apidoc -f '^[^.]+\\.ts$' -i lambda/ -e lambda/__tests__ -e lambda/node_modules  -o lambda_api_doc/",
    "build": "npx tsc && npm run apidoc",
    "cdk": "env-cmd cdk",
    "deploy": "npm run build && env-cmd cdk deploy",
    "deploy-local": "env-cmd cdk synth --no-staging >| template.yaml && sam local start-api",
    "lint": "tsc --noEmit && eslint '*/**/*.ts' --quiet --fix",
    "lint-dry-run": "npx tsc --noEmit && eslint '*/**/*.ts' --fix-dry-run",
    "post-legacy": "env-cmd ts-node tools/post-legacy.ts",
    "test": "jest",
    "test-coverage": "jest --collectCoverage",
    "watch": "npx tsc -w"
  },
  "devDependencies": {
    "@aws-cdk/assert": "^1.41.0",
    "@types/aws-lambda": "^8.10.51",
    "@types/cheerio": "^0.22.18",
    "@types/form-data": "^2.5.0",
    "@types/jest": "^24.9.1",
    "@types/mime-types": "^2.1.0",
    "@types/node": "^13.13.9",
    "@types/parse5": "^5.0.3",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "aws-cdk": "^1.41.0",
    "aws-sdk-mock": "^5.1.0",
    "env-cmd": "^10.1.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-config-airbnb-typescript": "^6.3.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jest": "^23.13.1",
    "eslint-plugin-prettier": "^3.1.2",
    "form-data": "^3.0.0",
    "fs": "0.0.1-security",
    "jest": "^24.9.0",
    "parse5": "^5.1.1",
    "prettier": "^1.19.1",
    "ts-jest": "^25.5.1",
    "ts-node": "^8.10.1",
    "typescript": "^3.9.3"
  },
  "dependencies": {
    "@aws-cdk/aws-apigateway": "^1.41.0",
    "@aws-cdk/aws-certificatemanager": "^1.41.0",
    "@aws-cdk/aws-iam": "^1.41.0",
    "@aws-cdk/aws-lambda": "^1.41.0",
    "@aws-cdk/aws-s3": "^1.41.0",
    "@aws-cdk/core": "^1.41.0",
    "axios": "^0.19.2",
    "cheerio": "^1.0.0-rc.3",
    "mime-types": "^2.1.27"
  },
  "jest": {
    "moduleFileExtensions": [
      "js"
    ]
  }
}
