    vertical-align: baseline;   /* cascaded environment */
    text-decoration: none;      /* cascaded environment */
}
.variantrefs:before { content: "(" }
.variantrefs:after { content: ") " }
.variantrefs>.xitem + .xitem:before { content: "; " }

